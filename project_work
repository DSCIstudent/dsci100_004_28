   "outputs": [],
   "source": [
    "summed_data_split <- initial_split(summed_data, prop = 0.7, strata = age)\n",
    "summed_data_training <- training(summed_data_split)\n",
    "summed_data_testing <- testing(summed_data_split)"
   ]
